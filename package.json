    "ftp": "^0.3.9",
    "MD5": "^1.2.1",
    "util": "*",
    "node-dir": "*",
    "bluebird": "^2.9.16",
    "expand-home-dir": "^0.0.2",
    "atom-space-pen-views": "^2.0.3"
  },
  "devDependencies": {
    "should": "latest",
    "coveralls": "2.3.0"
  },
  "keywords": [
    "atom",
    "package",
    "sftp",
    "ftp",
    "remote",
    "deployment"
  ],
  "license": "MIT"
}
