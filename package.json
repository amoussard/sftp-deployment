    "sftp-deployment:downloadSelection",
    "core:save"
  ],
  "engines": {
    "atom": ">0.50.0"
  },
  "scripts": {
    "test": "make test"
  },
  "dependencies": {
    "ssh2": "^0.2.23",
    "ftp": "*",
    "MD5": "^1.2.1",
    "util": "*",
    "node-dir": "*",
    "bluebird": "^2.9.16"
  },
  "devDependencies": {
    "should": "latest",
    "coveralls": "2.3.0"
  },
  "keywords": [
    "atom",
    "package",
    "sftp",
    "ftp",
    "remote",
    "deployment"
  ],
  "license": "MIT"
}
